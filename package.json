{
  "name": "base64-esm",
  "type": "module",
  "version": "0.1.2",
  "packageManager": "pnpm@7.9.5",
  "description": "A Base64 library written in ESM.",
  "author": "mys1024",
  "license": "MIT",
  "homepage": "https://github.com/mys1024/base64-esm#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/mys1024/base64-esm.git"
  },
  "bugs": {
    "url": "https://github.com/mys1024/base64-esm/issues"
  },
  "keywords": [
    "base64",
    "base64url",
    "encode",
    "decode",
    "string",
    "Uint8Array",
    "esm",
    "es6"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "test": "vitest run",
    "test:dev": "vitest",
    "test:cover": "vitest --coverage",
    "publish": "pnpm test && pnpm build && pnpm publish"
  },
  "devDependencies": {
    "@vitest/coverage-c8": "^0.22.1",
    "unbuild": "^0.8.9",
    "vitest": "^0.22.1"
  }
}
